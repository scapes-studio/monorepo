# Kamal Example Environment (Edit this file into `.env.production / .env.local`)
# Usage: set -a && source .env.production && set +a && kamal setup

# Ethereum RPC endpoints
PONDER_RPC_URL_1=
PONDER_RPC_URL_1_WS=

# OpenSea API key
OPENSEA_API_KEY=

# PostgreSQL password (set a strong password)
POSTGRES_PASSWORD=

# Database URL (uses Kamal accessory hostname)
DATABASE_URL=

# Ponder auto generated static view
PONDER_VIEWS_SCHEMA=

# Don't send metrics to ponder
PONDER_TELEMETRY_DISABLED=

# Used for data imports
PUNKSCAPE_DATABASE_URL=

# S3 Bucket
S3_KEY=
S3_SECRET=
S3_BUCKET=
S3_REGION=
S3_ENDPOINT=
S3_PUBLIC_URL=

# Used for ai image generation
TEAM_TOKEN=
LEONARDO_KEY=

# Used for gallery27 signing
G27V1_SIGNER_PRIVATE_KEY=
G27_SIGNER_PRIVATE_KEY=

# Pinata IPFS service
PINATA_KEY=
PINATA_SECRET=
PINATA_JWT=
PINATA_GATEWAY=

# Notification Settings
SKIP_NOTIFICATIONS=

# Discord webhooks (for bot notifications)
DISCORD_WEBHOOK_MAIN=
DISCORD_WEBHOOK_LISTINGS=
DISCORD_WEBHOOK_G27=

# Twitter API - Scapes account (for bot notifications)
TWITTER_CONSUMER_KEY=
TWITTER_CONSUMER_SECRET=
TWITTER_OAUTH_TOKEN=
TWITTER_OAUTH_TOKEN_SECRET=

# Twitter API - Gallery27 account (for bot notifications)
G27_TWITTER_CONSUMER_KEY=
G27_TWITTER_CONSUMER_SECRET=
G27_TWITTER_OAUTH_TOKEN=
G27_TWITTER_OAUTH_TOKEN_SECRET=

# Deployment Variables
DOCKER_REGISTRY_USERNAME=
DEPLOY_HOST=
INDEXER_HOST=
